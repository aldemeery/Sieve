{
    "name": "aldemeery/sieve",
    "description": "A simple, clean and elegant way to filter Eloquent models.",
    "keywords": ["laravel", "eloquent", "filters", "search", "query", "filter", "filtration"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Osama Aldemeery",
            "email": "aldemeery@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "illuminate/support": "~5.8|^6.0|^7.0|^8.0",
        "illuminate/database": "~5.8|^6.0|^7.0|^8.0",
        "illuminate/http": "~5.8|^6.0|^7.0|^8.0",
        "illuminate/console": "~5.8|^6.0|^7.0|^8.0",
        "laravel/helpers": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Aldemeery\\Sieve\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aldemeery\\Sieve\\FiltersServiceProvider"
            ]
        }
    }
}
